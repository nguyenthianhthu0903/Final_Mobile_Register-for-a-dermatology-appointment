<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Final"
        android:supportsRtl="true"
        android:theme="@style/Theme.Final"
        tools:targetApi="31">

        <activity
            android:name=".Activity.CheckLoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activity.CheckRole"
            android:exported="false">
        </activity>

        <activity
            android:name=".Doctor.BottomNavigationBar"
            android:exported="false">
        </activity>

        <activity
            android:name=".Doctor.DoctorLogin"
            android:exported="false">
        </activity>

        <activity
            android:name=".Patient.PatientBottomNavigationBar"
            android:exported="false">
        </activity>

        <activity
            android:name=".Patient.PatientLogin"
            android:exported="false">
        </activity>

        <activity
            android:name=".Patient.PatientRegister"
            android:exported="false">
        </activity>

    </application>

</manifest>